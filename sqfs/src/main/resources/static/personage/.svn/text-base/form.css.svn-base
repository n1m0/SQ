
input.text {
    border: 1px solid #b9b9b9;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    height: 36px;
    line-height: 36px;
    padding: 0 12px;
    font-size: 14px;
    color: #666;
    width: 228px;
}
input.sm-text {
    width: 115px;
}
input.text.valid {
     border: 1px solid #58b039 !important;
}
input.text.error {
    border: 1px solid #dc4d3b !important;
}
input.text:focus {
     border: 1px solid #58b039;
}
input.text-r {
    padding: 10px 5px;
    border: 1px solid #b9b9b9;
    border-top-left-radius: 3px;
    -moz-border-top-left-radius: 3px;
    -webkit-border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    -moz-border-bottom-left-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    float: left;
    border-right: none;
    width: 220px;
    height/*\**/: 19px \9;
}
div.text-r {
    border-top-right-radius: 3px;
    -moz-border-top-right-radius: 3px;
    -webkit-border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    -moz-border-bottom-right-radius: 3px;
    -webkit-border-bottom-right-radius: 3px;
    height: 39px;
    width: 48px;
    border: 1px solid #9faab2;
    background: #9faab2;
    float: left;
    text-align: center;
    line-height: 39px;
    color: #FFF;
}
.input.textarea {
    width: 400px;
    height: 100px;
     border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border: 1px solid #CCC;
}
.radio {
    height: 36px;
    border: 1px solid #b9b9b9;
    display: inline-block;
    /*background: #f5f5f5;*/
    padding: 0 32px;
    font-size: 14px;
    color: #666666;
    line-height: 36px;
    cursor: pointer;
    float: left;
    margin-right: 9px;
    border-radius: 3px;
}
.radio:hover {
    border-color:#96c32a;
    color: #96c32a;
}
.radio.active{
     background-color: #96c32a;
     border-color: #96c32a;
     color: #FFF;
}
.radio input {
    position: absolute;
    top: -1000px;
}
.form-grounp {
    margin-bottom: 0;  
}
form .errormsg {
    text-align: left !important;
    background-position: 0 5px;
    background-repeat:no-repeat;
    padding-left: 20px;
    margin-left: 100px;
    height: 26px;
    line-height: 26px;
    width: auto !important;
    display: block;
    color: #dc4d3b;
    font-size: 12px;
}
.u-form .errormsg{
    margin-left: 146px !important;
}
.msgnav {
    height: 26px;
    overflow: hidden;
}
.form-grounp .uploadimg{float: left;}
.form-grounp .uploadimg .imgNav{width: 173px;height: 107px;border:2px dashed #d9dde3;border-radius: 6px;-webkit-border-radius:6px;-moz-border-radius:6px;float: left;position: relative;overflow: hidden;}
.form-grounp .uploadimg .imgNav img{width: 100%;height: 100%;display: none;}
.form-grounp .uploadimg .imgNav input[type="file"]{opacity: 0; -moz-opacity: 0; filter: alpha(opacity=0);position: absolute;width: 100%;height: 100%;top: 0;left: 0;cursor: pointer;}
.form-grounp .uploadimg .imgNav .tit{color: #c8c8c8;font-size: 15px;text-align: center;margin-top: 20px;}
.form-grounp .uploadimg .imgNav .nodata{cursor: pointer;}
.form-grounp .uploadimg .imgNav .add{width: 25px;height: 7px;background: #d4d4d4;border-radius:30px;-webkit-border-radius:30px;-moz-border-radius:30px;margin:30px auto;}
.form-grounp .uploadimg .imgNav .add .fg{width: 7px;height: 25px;background: #d4d4d4;border-radius: 30px;-webkit-border-radius:30px;-moz-border-radius:30px;margin:0 auto;top: -9px;position: relative;}
.form-grounp .uploadimg .imgalt{float: left;margin-left: 45px;}
.form-grounp .uploadimg .imgalt > .tit{color: #4782b8;font-size: 14px;height: 30px;line-height: 30px;}
.form-grounp .uploadimg .imgalt > .label{font-size: 14px;color: #444444;line-height: 23px;}